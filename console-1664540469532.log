function Person(name, age, phone){
    this.name = name;
    this.age = age;
    this.phone = phone;
}
Person.prototype.print = function(){
    console.log('Personal Info ');
    console.log('Name ',this.name);
    console.log('Age ', this.age);
    console.log('Phone ', this.phone);
}
//var ram = new Person('Amit', 21, 2222);
// Function Constructor - Intialize Members
function Employee(id, salary, bonus, name, age, phone){
    // this - It belongs to Employee object
    Person.call(this, name, age, phone); // super call
    this.id = id;
    this.salary = salary;
    this.bonus = bonus;
}
Employee.prototype.print = function(){
    Person.prototype.print.call(this);
    console.log('Employee Info ');
    console.log('Id ',this.id);
    console.log('Salary ', this.salary);
    console.log('Bonus ', this.bonus);
}
